from flask import Flask
app = Flask(__name__)

@app.route("/")
def hello_world():
    return "No hablo queso!"

@app.route("/second")
def speech():
    return "What is your name?"

@app.route("/introduction")
def introduce():
    return "Hi, my name is Zane."

app.run()
